{
  "compilerOptions": {
    "target": "es6",
    "module": "es6",
    "baseUrl": ".",
    "paths": {
      "ES": [
        "./src/es/index.js",
        "./src/es/index.mjs"
      ],
      "ES/*": [
        "./src/es/*"
      ],
      "CJS": [
        "./src/cjs/index.js",
        "./src/cjs/index.cjs"
      ],
      "CJS/*": [
        "./src/cjs/*"
      ],
      "TS": [
        "./src/ts/index.ts"
      ],
      "TS/*": [
        "./src/ts/*"
      ],
      "MobiusUtils": [
        "./src/ts/libs/mobius-utils.ts"
      ],
      "MobiusUI": [
        "./src/ts/libs/mobius-ui.ts"
      ],
      "MobiusJS": [
        "./src/ts/libs/mobius-js.ts"
      ],
      "Business/*": [
        "./src/ts/business/*"
      ],
      "FreeBusiness/*": [
        "./src/ts/business-free/*"
      ],
      "Interface/*": [
        "./src/ts/interface/*"
      ],
      "Libs/*": [
        "./src/ts/libs/*"
      ],
      "Statics/*": [
        "./src/statics/*"
      ],
      "Images/*": [
        "./src/statics/images/*"
      ],
      "Styles/*": [
        "./src/statics/styles/*"
      ]
    },
    "plugins": [
      {
        "name": "typescript-lit-html-plugin",
        "tags": [
          "template",
          "raw",
          "mark",
          "style",
          "actuate",
          "view",
          "html"
        ]
      }
    ]
  },
  "exclude": [
    "node_modules",
    "dist",
    "build",
    "dev",
    "release",
    "scripts"
  ]
}
