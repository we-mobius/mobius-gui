{
  "name": "@we-mobius/mobius-ui",
  "version": "0.1.0",
  "description": "Mobius UI is a neumorphism-like, utility-first UI framework.",
  "main": "./src/index.js",
  "keywords": [
    ""
  ],
  "scripts": {
    "cz-commit": "npm run lint && npx git-cz",
    "lint": "npx eslint .",
    "lint:fix": "npx eslint . --fix",
    "dev": "webpack-dev-server --env.mode=development",
    "build": "webpack --env.mode=development",
    "dist": "webpack --env.mode=production",
    "serve": "live-server ./dist --port=0624",
    "expose": "ngrok http 624",
    "bundle": "webpack --env.mode=release && node ./bin/release"
  },
  "repository": "https://github.com/we-mobius/mobius-ui.git",
  "author": {
    "name": "kongxiangyan",
    "email": "kcigaret@outlook.com",
    "url": "https://cigaret.world/"
  },
  "license": "GPL v3.0",
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@commitlint/cli": "^8.3.5",
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "autoprefixer": "^9.8.5",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.1.2",
    "commitlint-config-cz": "^0.13.1",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.6.0",
    "cssnano": "^4.1.10",
    "cz-customizable": "^6.2.1",
    "eslint": "^6.8.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "file-loader": "^6.0.0",
    "handlebars": "^4.7.6",
    "handlebars-loader": "^1.7.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "mini-css-extract-plugin": "^0.9.0",
    "mini-svg-data-uri": "^1.2.3",
    "postcss": "^7.0.32",
    "postcss-advanced-variables": "^3.0.1",
    "postcss-extend-rule": "^3.0.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "standard": "^14.3.4",
    "style-loader": "^1.2.1",
    "tailwindcss": "^1.6.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".cz-config.js"
    }
  },
  "dependencies": {
    "@cycle/dom": "^22.7.0",
    "@cycle/isolate": "^5.2.0",
    "@cycle/rxjs-run": "^10.5.0",
    "rxjs": "^6.6.0"
  },
  "jsdelivr": "./release/styles/mobius.css",
  "files": [
    "/release/*",
    "/public/*",
    "/dist/*",
    "/docs/*"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  }
}
